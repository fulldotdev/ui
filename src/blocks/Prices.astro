---
import Section from 'fulldev-ui/components/Section.astro'

const { ...rest } = Astro.props
---

<Section
  class:list="prices"
  structure="grid"
  frame="panel"
  {...rest}
/>

<style is:global lang="scss">
  @layer fulldev {
    .prices {
      .card {
        .wrap {
          margin-top: auto;
        }
      }
    }
  }
</style>
