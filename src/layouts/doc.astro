---
import site from "@/data/site.json"

import { type EntryProps } from "@/lib/types"
import Layout from "@/components/layout.astro"
import Doc1 from "@/blocks/doc-1"

type Props = EntryProps

const { seo, blocks, ...props } = Astro.props
---

<Layout {...props} {...seo} blocks={blocks}>
  <Doc1 menus={site.header.menus} {...props}>
    <slot />
  </Doc1>
</Layout>
