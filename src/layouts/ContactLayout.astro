---
import type { ComponentProps } from 'astro/types'
import Sections from 'fulldev-ui/components/sections.astro'
import Layout from 'fulldev-ui/layouts/Layout.astro'
import Contact from '../blocks/Contact.astro'

type Props = ComponentProps<typeof Layout> & {
  contactProps: ComponentProps<typeof Contact>
}

const { render, sections, contactProps, ...rest } = Astro.props
---

<Layout {...Astro.props}>
  <Contact
    level={1}
    {...rest}
  />
  <Sections {sections} />
  <slot />
</Layout>
