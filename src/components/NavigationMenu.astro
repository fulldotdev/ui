---
import { NavigationMenu as NavigationMenuComposite } from '@/components/composite/navigation-menu'
import type { ComponentProps } from 'astro/types'

interface Props extends ComponentProps<typeof NavigationMenuComposite> {}

const { items, ...rest } = Astro.props
---

{
  items && (
    <NavigationMenuComposite
      client:load
      {items}
      {...rest}
    />
  )
}
