---
import { Carousel as CarouselComposite } from '@/components/composite/carousel'
import type { ComponentProps } from 'astro/types'

interface Props extends ComponentProps<typeof CarouselComposite> {}

const slot = await Astro.slots.render('default')
---

{
  slot.trim().length && (
    <CarouselComposite client:load>
      <Fragment set:html={slot} />
    </CarouselComposite>
  )
}
