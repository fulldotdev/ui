---
import { Form as FormComposite } from '@/components/composite/form'
import type { ComponentProps } from 'astro/types'

interface Props extends ComponentProps<typeof FormComposite> {}

const { items, ...rest } = Astro.props
---

{
  items && (
    <FormComposite
      items={items}
      {...rest}
    />
  )
}
