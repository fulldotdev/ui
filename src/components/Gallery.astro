---
import { Gallery as GalleryComposite } from '@/components/composite/gallery'
import type { ComponentProps } from 'astro/types'

interface Props extends ComponentProps<typeof GalleryComposite> {}

const slot = await Astro.slots.render('default')
---

{
  slot.trim().length && (
    <GalleryComposite client:load>
      <Fragment set:html={slot} />
    </GalleryComposite>
  )
}
