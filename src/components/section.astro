---
import { type BlockSchema } from "@/schemas/block"
import Articles from "@/components/blocks/articles.astro"
import Blocks from "@/components/blocks/blocks.astro"
import Collections from "@/components/blocks/collections.astro"
import Contact from "@/components/blocks/contact.astro"
import Content from "@/components/blocks/content.astro"
import Cta from "@/components/blocks/cta.astro"
import Faqs from "@/components/blocks/faqs.astro"
import Feature from "@/components/blocks/feature.astro"
import Features from "@/components/blocks/features.astro"
import Jobs from "@/components/blocks/jobs.astro"
import Persons from "@/components/blocks/persons.astro"
import Pricings from "@/components/blocks/pricings.astro"
import Products from "@/components/blocks/products.astro"
import Quote from "@/components/blocks/quote.astro"
import Reviews from "@/components/blocks/reviews.astro"
import Services from "@/components/blocks/services.astro"

type Props = BlockSchema

const props = Astro.props
---

{props.type === "articles" && <Articles {...props} />}
{props.type === "blocks" && <Blocks {...props} />}
{props.type === "collections" && <Collections {...props} />}
{props.type === "contact" && <Contact {...props} />}
{props.type === "content" && <Content {...props} />}
{props.type === "cta" && <Cta {...props} />}
{props.type === "faqs" && <Faqs {...props} />}
{props.type === "feature" && <Feature {...props} />}
{props.type === "features" && <Features {...props} />}
{props.type === "jobs" && <Jobs {...props} />}
{props.type === "persons" && <Persons {...props} />}
{props.type === "pricings" && <Pricings {...props} />}
{props.type === "products" && <Products {...props} />}
{props.type === "quote" && <Quote {...props} />}
{props.type === "reviews" && <Reviews {...props} />}
{props.type === "services" && <Services {...props} />}
