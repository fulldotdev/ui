---
import { Icon as AstroIcon } from 'astro-icon/components'
import type { ComponentProps } from 'astro/types'

interface Props extends ComponentProps<typeof AstroIcon> {}

const { name, ...rest } = Astro.props
---

{
  name && (
    <AstroIcon
      name={'tabler:' + name}
      {...rest}
    />
  )
}
