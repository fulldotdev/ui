---
import { Search as SearchComposite } from '@/components/composite/search'
import type { ComponentProps } from 'react'

interface Props extends ComponentProps<typeof SearchComposite> {}

const { groups, ...rest } = Astro.props
---

{
  groups && (
    <SearchComposite
      client:load
      groups={groups}
      {...rest}
    />
  )
}
