---
import { Block } from "@/components/block"

const { block, blocks, ...page } = Astro.props
---

{
  block && (
    <Block client:load {...page}>
      <slot />
    </Block>
  )
}

{
  blocks?.map((block: any) => (
    <Block client:load {...block}>
      <Fragment set:html={block.children} />
    </Block>
  ))
}
