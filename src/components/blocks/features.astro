---
import { type FeaturesSchema } from "@/schemas/blocks/features"
import Features1 from "@/components/blocks/features-1"

type Props = FeaturesSchema

const { variant = "1", html, ...block } = Astro.props
---

{
  variant === "1" && (
    <Features1 {...block}>
      <Fragment set:html={html} />
    </Features1>
  )
}
