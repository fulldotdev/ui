---
import { type PricingsSchema } from "@/schemas/blocks/pricings"
import Pricings1 from "@/components/blocks/pricings-1"

type Props = PricingsSchema

const { variant, html, ...props } = Astro.props
---

{
  variant === "1" && (
    <Pricings1 {...props}>
      <Fragment set:html={html} />
      <slot />
    </Pricings1>
  )
}
