---
import { type QuoteSchema } from "@/schemas/blocks/quote"
import Quote1 from "@/components/blocks/quote-1"
import Quote2 from "@/components/blocks/quote-2"

type Props = QuoteSchema

const { variant, ...props } = Astro.props
---

{variant === "1" && <Quote1 client:load {...props} />}
{variant === "2" && <Quote2 client:load {...props} />}
