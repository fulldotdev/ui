---
import { type BannerSchema } from "@/schemas/blocks/banner"
import Banner1 from "@/components/blocks/banner-1"

type Props = BannerSchema

const { variant = "1", html, ...block } = Astro.props
---

{
  variant === "1" && (
    <Banner1 {...block}>
      <Fragment set:html={html} />
    </Banner1>
  )
}
