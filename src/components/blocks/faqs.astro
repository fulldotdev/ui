---
import { type FaqsSchema } from "@/schemas/blocks/faqs"
import Faqs1 from "@/components/blocks/faqs-1"

type Props = FaqsSchema

const { variant = "1", html, ...block } = Astro.props
---

{
  variant === "1" && (
    <Faqs1 client:load {...block}>
      <Fragment set:html={html} />
    </Faqs1>
  )
}
