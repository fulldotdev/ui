{
  "extends": "astro/tsconfigs/strictest",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "preserve",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "fulldev-ui/*": [
        "src/*"
      ],
      "fulldev-ui/lib/utils": [
        "src/shadcn/lib/utils.ts"
      ]
    }
  }
}