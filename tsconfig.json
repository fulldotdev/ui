{
  "extends": "astro/tsconfigs/strictest",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "baseUrl": ".",
    "paths": {
      "fulldev-ui/*": ["src/*"],
      "@/*": ["src/shadcn/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
