{
  "name": "fulldev-ui",
  "version": "0.6.4",
  "description": "A component library built for Astro",
  "homepage": "https://ui.full.dev",
  "bugs": "https://github.com/fulldotdev/ui/issues",
  "license": "MIT",
  "author": "Fulldev <contact@full.dev> (https://full.dev)",
  "repository": "github:fulldotdev/ui",
  "keywords": [
    "astro-integration",
    "withastro",
    "astro",
    "astro-component",
    "astro-component-library",
    "astro-ui-library",
    "astro-ui"
  ],
  "scripts": {
    "astro": "astro",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "check": "astro check",
    "preview": "astro preview",
    "build:prod": "astro check && astro build",
    "test": "astro check && astro build && astro preview --host",
    "l": "pnpm link --global"
  },
  "type": "module",
  "exports": {
    "./*": "./src/*",
    ".": "./src/integrations/index.ts",
    "./integration": "./src/integrations/index.ts",
    "./[...page].astro": "./src/pages/[...page].astro",
    "./[categories]/[...category].astro": "./src/pages/[categories]/[...category].astro",
    "./[docs]/[...doc].astro": "./src/pages/[docs]/[...doc].astro",
    "./[jobs]/[...job].astro": "./src/pages/[jobs]/[...job].astro",
    "./[posts]/[...post].astro": "./src/pages/[posts]/[...post].astro",
    "./[products]/[...product].astro": "./src/pages/[products]/[...product].astro"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.0.7",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/tailwind": "^5.1.5",
    "@iconify-json/tabler": "^1.2.15",
    "astro": "^5.1.9",
    "astro-live-code": "^0.0.5",
    "astro-robots-txt": "^1.0.0",
    "typescript": "^5.7.3",
    "vite": "^6.0.11"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@types/js-yaml": "^4.0.9",
    "prettier": "3.4.2",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-astro-organize-imports": "^0.4.11",
    "prettier-plugin-css-order": "^2.1.2",
    "prettier-plugin-organize-imports": "^4.1.0"
  }
}
