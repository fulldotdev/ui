    "astro",
    "astro-component",
    "astro-component-library",
    "astro-ui-library",
    "astro-ui"
  ],
  "type": "module",
  "scripts": {
    "astro": "astro",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "check": "astro check",
    "preview": "astro preview",
    "build:prod": "astro check && astro build",
    "test": "astro check && astro build && astro preview --host",
    "shadcn": "pnpm dlx shadcn@canary add",
    "registry:build": "shadcn build",
    "l": "pnpm link --global fulldev-ui"
  },
  "bin": {
    "fulldev-ui": "cli.js"
  },
  "files": [
