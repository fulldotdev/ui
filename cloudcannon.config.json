{
  "_schema": "_schema",
  "source": "/",
  "paths": {
    "uploads": "public/images",
    "static": "public"
  },
  "collections_config": {
    "pages": {
      "path": "src/content/pages",
      "name": "Pagina's",
      "icon": "web",
      "url": "/[full_slug]/",
      "disable_url": false,
      "_enabled_editors": ["visual", "content"],
      "disable_add": false,
      "disable_add_folder": false,
      "disable_file_actions": true,
      "schemas": {
        "default": {
          "path": ".cloudcannon/schemas/page.md"
        }
      }
    },
    "services": {
      "path": "src/content/services",
      "name": "Aanbod",
      "icon": "category",
      "url": "/aanbod/[full_slug]/",
      "disable_url": false,
      "_enabled_editors": ["visual", "content"],
      "disable_add": false,
      "disable_add_folder": false,
      "disable_file_actions": true,
      "schemas": {
        "default": {
          "path": ".cloudcannon/schemas/page.md"
        }
      }
    },
    "persons": {
      "path": "src/content/persons",
      "url": "/team/[full_slug]/",
      "name": "Personen",
      "icon": "person",
      "disable_url": false,
      "disable_add": false,
      "disable_add_folder": true,
      "_enabled_editors": ["data", "visual"],
      "schemas": {
        "default": {
          "path": ".cloudcannon/schemas/person.md"
        }
      }
    },
    "events": {
      "path": "src/content/events",
      "url": "/groepslessen/[full_slug]/",
      "name": "Groepslessen",
      "icon": "calendar_today",
      "disable_url": false,
      "disable_add": false,
      "disable_add_folder": true,
      "_enabled_editors": ["data", "visual"],
      "schemas": {
        "default": {
          "path": ".cloudcannon/schemas/event.md"
        }
      }
    },
    "policies": {
      "path": "src/content/policies",
      "name": "Beleid",
      "icon": "policy",
      "url": "/beleid/[full_slug]/",
      "disable_url": false,
      "_enabled_editors": ["content"],
      "disable_add": false,
      "disable_add_folder": true,
      "disable_file_actions": true,
      "schemas": {
        "default": {
          "path": ".cloudcannon/schemas/policy.md"
        }
      }
    },
    "docs": {
      "path": "src/content/docs",
      "name": "Docs",
      "icon": "docs",
      "url": "/docs/[full_slug]/",
      "disable_url": false,
      "_enabled_editors": ["content"],
      "disable_add": false,
      "disable_add_folder": true,
      "disable_file_actions": true,
      "schemas": {
        "default": {
          "path": ".cloudcannon/schemas/doc.md"
        }
      }
    },
    "reviews": {
      "path": "src/content/reviews",
      "name": "Reviews",
      "icon": "reviews",
      "disable_url": false,
      "disable_add": false,
      "disable_add_folder": true,
      "disable_file_actions": true,
      "_enabled_editors": ["data"],
      "schemas": {
        "default": {
          "path": ".cloudcannon/schemas/review.md"
        }
      }
    },
    "data": {
      "name": "Settings",
      "path": "src/data",
      "icon": "settings",
      "disable_file_actions": true,
      "disable_add_folder": true,
      "disable_add": true,
      "_enabled_editors": ["data"]
    }
  },
  "collection_groups": [
    {
      "heading": "Pagina's",
      "collections": ["pages", "services", "events", "persons", "policies"]
    },
    {
      "heading": "Data",
      "collections": ["reviews", "data"]
    }
  ],
  "_inputs": {
    "links": {
      "type": "object",
      "options": {
        "structures": "_structures.link"
      },
      "cascade": true,
      "subtype": "object"
    },
    "avatar": {
      "type": "object",
      "options": {
        "structures": "_structures.image"
      },
      "cascade": true,
      "subtype": "object"
    },
    "avatars": {
      "type": "array",
      "options": {
        "structures": "_structures.image"
      },
      "cascade": true
    },
    "logo": {
      "type": "object",
      "options": {
        "structures": "_structures.image"
      },
      "cascade": true,
      "subtype": "object"
    },
    "logos": {
      "type": "array",
      "options": {
        "structures": "_structures.image"
      },
      "cascade": true
    },
    "src": {
      "type": "image",
      "options": {
        "mime_type": "image/webp",
        "accepts_mime_types": "image/jpeg,image/jpg,image/png,image/webp",
        "resize_style": "contain",
        "width": 1920,
        "height": 1920
      },
      "cascade": true
    },
    "avatar.src": {
      "type": "image",
      "options": {
        "mime_type": "image/webp",
        "accepts_mime_types": "image/jpeg,image/jpg,image/png,image/webp,image/svg+xml",
        "resize_style": "cover",
        "width": 100,
        "height": 100
      },
      "cascade": true
    },
    "avatars[*].src": {
      "type": "image",
      "options": {
        "mime_type": "image/webp",
        "accepts_mime_types": "image/jpeg,image/jpg,image/png,image/webp,image/svg+xml",
        "resize_style": "cover",
        "width": 100,
        "height": 100
      },
      "cascade": true
    },
    "logo.src": {
      "type": "image",
      "options": {
        "mime_type": "image/webp",
        "accepts_mime_types": "image/jpeg,image/jpg,image/png,image/webp,image/svg+xml",
        "resize_style": "contain",
        "width": 200,
        "height": 36
      },
      "cascade": true
    },
    "logos[*].src": {
      "type": "image",
      "options": {
        "mime_type": "image/webp",
        "accepts_mime_types": "image/jpeg,image/jpg,image/png,image/webp,image/svg+xml",
        "resize_style": "contain",
        "width": 200,
        "height": 36
      },
      "cascade": true
    },
    "socials": {
      "type": "array"
    },
    "socials[*]": {
      "type": "url"
    },
    "block": {
      "type": "auto",
      "hidden": true,
      "cascade": true
    },
    "reverse": {
      "type": "auto",
      "hidden": true,
      "cascade": true
    },
    "seo": {
      "type": "object",
      "subtype": "object"
    },
    "head": {
      "type": "code",
      "options": {
        "min_visible_lines": 10,
        "max_visible_lines": 30,
        "tab_size": 2,
        "theme": "monokai",
        "show_gutter": true,
        "syntax": "html"
      },
      "cascade": true
    },
    "body": {
      "type": "code",
      "options": {
        "min_visible_lines": 10,
        "max_visible_lines": 30,
        "tab_size": 2,
        "theme": "monokai",
        "show_gutter": true,
        "syntax": "html"
      },
      "cascade": true
    },
    "css": {
      "type": "code",
      "options": {
        "min_visible_lines": 10,
        "max_visible_lines": 30,
        "tab_size": 2,
        "theme": "monokai",
        "show_gutter": true,
        "syntax": "css"
      },
      "cascade": true
    },
    "href": {
      "type": "url",
      "cascade": true
    },
    "rating": {
      "type": "range",
      "options": {
        "min": 0,
        "max": 5,
        "step": 0.5
      },
      "cascade": true
    },
    "list": {
      "type": "array"
    },
    "list[*]": {
      "type": "text"
    },
    "children": {
      "type": "html",
      "options": {
        "allow_resize": true,
        "allow_custom_markup": false,
        "remove_custom_markup": false,
        "initial_height": 320,
        "blockquote": true,
        "bold": true,
        "bulletedlist": true,
        "center": null,
        "code": null,
        "copyformatting": null,
        "embed": null,
        "expandable": null,
        "format": "p h1 h2 h3",
        "height": null,
        "horizontalrule": null,
        "image": null,
        "image_size_attributes": false,
        "indent": null,
        "italic": true,
        "justify": null,
        "left": null,
        "link": null,
        "mime_type": null,
        "numberedlist": null,
        "outdent": null,
        "paths": null,
        "redo": null,
        "removeformat": null,
        "resize_style": null,
        "right": null,
        "snippet": null,
        "strike": null,
        "styles": null,
        "subscript": null,
        "superscript": null,
        "table": null,
        "underline": true,
        "undo": true,
        "width": null
      },
      "label": "Content",
      "cascade": true
    }
  },
  "_structures": {
    "link": {
      "style": "select",
      "values": [
        {
          "value": {
            "text": null,
            "href": null
          }
        }
      ]
    },
    "image": {
      "style": "select",
      "values": [
        {
          "value": {
            "src": null,
            "alt": null,
            "title": null
          }
        }
      ]
    },
    "seo": {
      "style": "select",
      "values": [
        {
          "value": {
            "title": null,
            "description": null,
            "image": null,
            "head": null,
            "body": null,
            "css": null
          }
        }
      ]
    }
  },
  "_editables": {
    "content": {
      "allow_custom_markup": false,
      "remove_custom_markup": false,
      "blockquote": true,
      "bold": true,
      "bulletedlist": true,
      "format": "p h1 h2 h3 h4 h5 h6",
      "height": 1920,
      "image": true,
      "image_size_attributes": true,
      "italic": true,
      "link": true,
      "mime_type": "image/webp",
      "numberedlist": true,
      "redo": true,
      "resize_style": "contain",
      "table": false,
      "underline": true,
      "undo": true,
      "width": 1920
    }
  },
  "timezone": "Etc/UTC"
}
